		"@sveltejs/kit": "^2.19.2",
		"@sveltejs/vite-plugin-svelte": "^4.0.4",
		"@tailwindcss/vite": "^4.1.11",
		"@types/eslint": "^8.56.12",
		"concurrently": "^8.2.2",
